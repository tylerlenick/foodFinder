    <div id="profile-div" class="container">
        <h1 id="userName" class="animated zoomIn">{{user.name}}</h1>
        <div class="row">
            <div class="col l4 m12 s12 center-align">
                <img id="profile-pic" class="center-align z-depth-5 responsive-img circle animated fadeInLeftBig" src="../images/Orion_male-user-white.png" alt="profile-pic">
            </div>
            <div id="bio-div" class="col l4 m12 s12 animated fadeInRightBig">
                <h6>Bio</h6>
                <div class="divider"></div>
                <ul id="user-info-list">
                    <li class="user-info">First Name: {{user.fname}}</li>
                    <li class="user-info">Last Name: {{user.lname}}</li>
                    <li class="user-info">Email: {{user.email}}</li>
                </ul>
            </div>
        </div>
        <div class="row">
            <div class="col l12 m12 s12 left-align">
                <a id="findres-user" href="/map" class="waves-effect waves-light btn pulse animated fadeInDownBig">Find A Restaurant!</a>
